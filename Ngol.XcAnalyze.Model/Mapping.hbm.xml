<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping
    xmlns="urn:nhibernate-mapping-2.2"
    assembly="Ngol.XcAnalyze.Model"
    namespace="Ngol.XcAnalyze.Model">
    <class
        name="State">
        <id
            name="Code" />
        <property
            name="Name"
            not-null="true"
            unique="true" />
    </class>
    <class
        name="City">
        <id
            name="ID">
            <generator
                class="native" />
        </id>
        <property
            name="Name"
            not-null="true" />
    </class>
</hibernate-mapping>